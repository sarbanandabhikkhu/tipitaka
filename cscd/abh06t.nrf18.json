    ],
    "innerText": "Samādhiṃ pana sābhiññaṃ, bhāvetvā tadanantaraṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bhāvetabbā yato paññā, bhikkhunā tena dhīmatā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
