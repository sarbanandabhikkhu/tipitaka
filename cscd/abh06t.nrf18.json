    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Nāmaṃ ",
      " nissāya rūpaṃ tu, rūpaṃ nissāya nāmakaṃ;"
    ],
    "innerText": "Nāmaṃ nissāya rūpaṃ tu, rūpaṃ nissāya nāmakaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pavattati sadā sabbaṃ, pañcavokārabhūmiyaṃ."
