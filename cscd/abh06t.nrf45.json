  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Ekamhā ",
      " pañcaviññāṇaṃ, pañcamhā sampaṭicchanaṃ;"
    ],
    "innerText": "Ekamhā pañcaviññāṇaṃ, pañcamhā sampaṭicchanaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sukhasantīraṇaṃ hoti, pañcavīsatito paraṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
