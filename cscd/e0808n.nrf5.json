    ],
    "innerText": "Sela’ntarā’yatana vāsika sīla’tthera,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Pādo garu gguṇagaru jjayataṃ mame’so;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Yassa ppabhāva’mavalamba maye’disopi,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sampādito’bhimata siddhikaro parattho."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
