    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Parijiṇṇamidaṃ ",
      " rūpaṃ, roganīḷaṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[roganiḍḍhaṃ (sī. pī.), roganiddhaṃ (syā.)]"
      },
      " pabhaṅguraṃ;"
