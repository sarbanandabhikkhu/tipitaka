    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yānimāni ",
      " apatthāni ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[yānimāni apatthāni (sī. syā. pī.), yānimāni’paviddhāni (?)]"
