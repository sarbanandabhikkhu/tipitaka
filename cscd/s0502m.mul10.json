  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Gahakāraka diṭṭhosi, puna gehaṃ na kāhasi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "children": [
      "Sabbā ",
      " te phāsukā bhaggā, gahakūṭaṃ visaṅkhataṃ;"
    ],
    "innerText": "Sabbā te phāsukā bhaggā, gahakūṭaṃ visaṅkhataṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Visaṅkhāragataṃ cittaṃ, taṇhānaṃ khayamajjhagā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
