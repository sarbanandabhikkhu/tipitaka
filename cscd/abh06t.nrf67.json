  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Iti ",
      " maggaphalaṭṭhānaṃ, vasā ariyapuggalā;"
    ],
    "innerText": "Iti maggaphalaṭṭhānaṃ, vasā ariyapuggalā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dvidhāpi catudhā yuggā, aṭṭha honti vibhāgato."
