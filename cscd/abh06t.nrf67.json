    ],
    "innerText": "Tato phalāni tīṇi dve, ekaṃ vātha yathākkamaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Maggāvasesanirodhamaggavuṭṭhānavīthiyaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
