[
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "chapter"
    },
    "innerText": "Aparagāthāsaṅgaṇikaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "1. Codanādipucchāvissajjanā"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "359"
      },
      "."
    ],
    "innerText": "359."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Codanā ",
      " kimatthāya, sāraṇā kissa kāraṇā;"
    ],
    "innerText": "Codanā kimatthāya, sāraṇā kissa kāraṇā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Saṅgho kimatthāya, matikammaṃ pana kissa kāraṇā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Codanā sāraṇatthāya, niggahatthāya sāraṇā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Saṅgho pariggahatthāya, matikammaṃ pana pāṭiyekkaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Mā kho turito abhaṇi, mā kho caṇḍikato bhaṇi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Mā kho paṭighaṃ janayi, sace anuvijjako tuvaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Mā kho sahasā abhaṇi, kathaṃ viggāhikaṃ anatthasaṃhitaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sutte vinaye anulome, paññatte anulomike."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Anuyogavattaṃ nisāmaya, kusalena buddhimatā kataṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Suvuttaṃ sikkhāpadānulomikaṃ, gatiṃ na nāsento samparāyikaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Hitesī anuyuñjassu, kālenatthūpasaṃhitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Cuditassa ca codakassa ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Sahasā vohāraṃ mā padhāresi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Codako āha āpannoti;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Cuditako āha anāpannoti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Ubho ",
      " anukkhipanto, paṭiññānusandhitena ",
      " kāraye;"
    ],
    "innerText": "Ubho anukkhipanto, paṭiññānusandhitena kāraye;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Paṭiññā lajjīsu katā, alajjīsu evaṃ na vijjati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Bahumpi alajjī bhāseyya, vattānusandhitena ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[vuttānusandhitena (sī. syā. ka.)]"
      },
      " kāraye."
    ],
    "innerText": "Bahumpi alajjī bhāseyya, vattānusandhitena [vuttānusandhitena (sī. syā. ka.)] kāraye."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Alajjī kīdiso hoti, paṭiññā yassa na rūhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Etañca ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[evañca (ka.)]"
      },
      " tāhaṃ pucchāmi, kīdiso vuccati alajjī puggalo."
    ],
    "innerText": "Etañca [evañca (ka.)] tāhaṃ pucchāmi, kīdiso vuccati alajjī puggalo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sañcicca āpattiṃ āpajjati, āpattiṃ parigūhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Agatigamanañca gacchati, ediso vuccati alajjīpuggalo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati alajjīpuggalo;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññañca tāhaṃ pucchāmi, kīdiso vuccati lajjīpuggalo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sañcicca āpattiṃ nāpajjati, āpattiṃ na parigūhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Agatigamanaṃ ",
      " na gacchati, ediso vuccati lajjīpuggalo."
    ],
    "innerText": "Agatigamanaṃ na gacchati, ediso vuccati lajjīpuggalo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Saccaṃ ",
      " ahampi jānāmi, ediso vuccati lajjīpuggalo;"
    ],
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati lajjīpuggalo;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññañca tāhaṃ pucchāmi, kīdiso vuccati adhammacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Akāle ",
      " codeti abhūtena;"
    ],
    "innerText": "Akāle codeti abhūtena;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Pharusena anatthasaṃhitena;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Dosantaro codeti no mettācitto;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ediso vuccati adhammacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati adhammacodako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññañca tāhaṃ pucchāmi, kīdiso vuccati dhammacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kālena codeti bhūtena, saṇhena atthasaṃhitena;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Mettācitto codeti no dosantaro;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ediso vuccati dhammacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Saccaṃ ",
      " ahampi jānāmi, ediso vuccati dhammacodako;"
    ],
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati dhammacodako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññañca tāhaṃ pucchāmi, kīdiso vuccati bālacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pubbāparaṃ na jānāti, pubbāparassa akovido;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Anusandhivacanapathaṃ na jānāti;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Anusandhivacanapathassa akovido;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ediso vuccati bālacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati bālacodako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Aññañca ",
      " tāhaṃ pucchāmi, kīdiso vuccati paṇḍitacodako."
    ],
    "innerText": "Aññañca tāhaṃ pucchāmi, kīdiso vuccati paṇḍitacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pubbāparampi jānāti, pubbāparassa kovido;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Anusandhivacanapathaṃ jānāti, anusandhivacanapathassa kovido;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ediso vuccati paṇḍitacodako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Saccaṃ ahampi jānāmi, ediso vuccati paṇḍitacodako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññañca tāhaṃ pucchāmi, codanā kinti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sīlavipattiyā codeti, atho ācāradiṭṭhiyā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ājīvenapi codeti, codanā tena vuccatīti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Aparaṃ gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
  }
]