  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tettiṃsa ceva dvattiṃsa, ekatiṃsa ca tiṃsa ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Ekadvattiṃsahīnā ca, tiṃsa dhammā",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "navajjake"
      },
      "."
    ],
    "innerText": "Ekadvattiṃsahīnā ca, tiṃsa dhammānavajjake."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "514"
      },
