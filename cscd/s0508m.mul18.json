  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Ārammaṇe ",
      " taṃ balasā nibandhisaṃ, nāgaṃva thambhamhi daḷhāya rajjuyā;"
    ],
    "innerText": "‘‘Ārammaṇe taṃ balasā nibandhisaṃ, nāgaṃva thambhamhi daḷhāya rajjuyā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Taṃ me suguttaṃ satiyā subhāvitaṃ, anissitaṃ sabbabhavesu hehisi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
