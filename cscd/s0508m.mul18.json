  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Tayā ",
      " sudantena avaṭṭhitena hi, hayena ",
      " yoggācariyova ujjunā;"
    ],
    "innerText": "‘‘Tayā sudantena avaṭṭhitena hi, hayena yoggācariyova ujjunā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pahomi maggaṃ paṭipajjituṃ sivaṃ, cittānurakkhīhi sadā nisevitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
