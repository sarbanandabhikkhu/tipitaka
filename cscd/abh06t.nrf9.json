    "innerText": "620."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Kaṭattāyeva ",
      " taṃ kammaṃ, phalassa pana paccayo;"
    ],
    "innerText": "Kaṭattāyeva taṃ kammaṃ, phalassa pana paccayo;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Na cassa vijjamānattaṃ, tassa vāvijjamānatā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
