    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Bhuñjati ",
      " anavajjāni, sāvajjāni na bhuñjati;"
    ],
    "innerText": "‘‘Bhuñjati anavajjāni, sāvajjāni na bhuñjati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ghāsamacchādanaṃ laddhā, sannidhiṃ parivajjayaṃ."
