  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pārājikā kāyikā dve, dve saṃvāsakabhūmiyo;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dvinnaṃ ratticchedo, paññattā dvaṅgulā duve."
