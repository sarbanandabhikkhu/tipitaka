    ],
    "innerText": "474. Kati āpattiyo kāyikā, kati vācasikā katā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Chādentassa kati āpattiyo, kati saṃsaggapaccayā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Chāpattiyo kāyikā, cha vācasikā katā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Chādentassa tisso āpattiyo, pañca saṃsaggapaccayā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Aruṇugge kati āpattiyo, kati yāvatatiyakā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Katettha aṭṭha vatthukā, katihi sabbasaṅgaho."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Aruṇugge tisso āpattiyo, dve yāvatatiyakā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ekettha aṭṭha vatthukā, ekena sabbasaṅgaho."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Vinayassa kati mūlāni, yāni buddhena paññattā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vinayagarukā kati vuttā, duṭṭhullacchādanā kati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Vinayassa dve mūlāni, yāni buddhena paññattā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vinayagarukā dve vuttā, dve duṭṭhullacchādanā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Gāmantare kati āpattiyo, kati nadipārapaccayā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Katimaṃsesu thullaccayaṃ, katimaṃsesu dukkaṭaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Gāmantare ",
      " catasso āpattiyo, catasso nadipārapaccayā;"
    ],
    "innerText": "Gāmantare catasso āpattiyo, catasso nadipārapaccayā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ekamaṃse thullaccayaṃ, navamaṃsesu dukkaṭaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kati vācasikā rattiṃ, kati vācasikā divā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dadamānassa kati āpattiyo, paṭiggaṇhantassa kittakā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Dve ",
      " vācasikā rattiṃ, dve vācasikā divā;"
    ],
    "innerText": "Dve vācasikā rattiṃ, dve vācasikā divā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dadamānassa tisso āpattiyo, cattāro ca paṭiggahe."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
