  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Aniyato’ti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Aniyato na niyato, anekaṃsikataṃ padaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tiṇṇamaññataraṃ ṭhānaṃ, ‘aniyato’ti pavuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Thullaccaya’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "children": [
      "Ekassa ",
      " mūle yo deseti, yo ca taṃ paṭigaṇhati;"
    ],
    "innerText": "Ekassa mūle yo deseti, yo ca taṃ paṭigaṇhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Accayo tena samo natthi, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Nissaggiya’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Saṅghamajjhe gaṇamajjhe, ekasseva ca ekato;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Nissajjitvāna deseti, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "‘Pācittiya’nti ",
      " yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
    ],
    "innerText": "‘Pācittiya’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Pāteti kusalaṃ dhammaṃ, ariyamaggaṃ aparajjhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Cittasaṃmohanaṭṭhānaṃ, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "‘Pāṭidesanīya’nti ",
      " yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
    ],
    "innerText": "‘Pāṭidesanīya’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Bhikkhu aññātako santo, kicchā laddhāya bhojanaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sāmaṃ gahetvā bhuñjeyya, ‘gārayha’nti pavuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Nimantanāsu bhuñjantā chandāya, vosāsati tattha bhikkhuniṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Anivāretvā tahiṃ bhuñje, gārayhanti pavuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Saddhācittaṃ ",
      " kulaṃ gantvā, appabhogaṃ anāḷiyaṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[anāḷhiyaṃ (sī. syā.)]"
      },
      ";"
    ],
    "innerText": "Saddhācittaṃ kulaṃ gantvā, appabhogaṃ anāḷiyaṃ [anāḷhiyaṃ (sī. syā.)];"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Agilāno tahiṃ bhuñje, gārayhanti pavuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Yo ce araññe viharanto, sāsaṅke sabhayānake;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aviditaṃ tahiṃ bhuñje, gārayhanti pavuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Bhikkhunī aññātikā santā, yaṃ paresaṃ mamāyitaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Sappi telaṃ madhuṃ phāṇitaṃ, macchamaṃsaṃ atho khīraṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Dadhiṃ sayaṃ ",
      " viññāpeyya bhikkhunī, gārayhapattā sugatassa sāsane."
    ],
    "innerText": "Dadhiṃ sayaṃ viññāpeyya bhikkhunī, gārayhapattā sugatassa sāsane."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Dukkaṭa’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aparaddhaṃ viraddhañca, khalitaṃ yañca dukkaṭaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Yaṃ manusso kare pāpaṃ, āvi vā yadi vā raho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "‘Dukkaṭa’nti pavedenti, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Dubbhāsita’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Dubbhāsitaṃ durābhaṭṭhaṃ, saṃkiliṭṭhañca yaṃ padaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yañca viññū garahanti, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘Sekhiya’nti yaṃ vuttaṃ, taṃ suṇohi yathātathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sekkhassa sikkhamānassa, ujumaggānusārino."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Ādi ",
      " cetaṃ caraṇañca, mukhaṃ saññamasaṃvaro;"
    ],
    "innerText": "Ādi cetaṃ caraṇañca, mukhaṃ saññamasaṃvaro;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sikkhā etādisī natthi, tenetaṃ iti vuccati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[udā. 45 udānepi]"
      },
      " Channamativassati ",
      ", vivaṭaṃ nātivassati;"
    ],
    "innerText": "[udā. 45 udānepi] Channamativassati , vivaṭaṃ nātivassati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tasmā channaṃ vivaretha, evaṃ taṃ nātivassati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Gati migānaṃ pavanaṃ, ākāso pakkhinaṃ gati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vibhavo gati dhammānaṃ, nibbānaṃ arahato gatīti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Gāthāsaṅgaṇikaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Tassuddānaṃ –"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sattanagaresu paññattā, vipatti caturopi ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "children": [
      "Bhikkhūnaṃ ",
      " bhikkhunīnañca, sādhāraṇā asādhāraṇā;"
    ],
    "innerText": "Bhikkhūnaṃ bhikkhunīnañca, sādhāraṇā asādhāraṇā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sāsanaṃ anuggahāya, gāthāsaṅgaṇikaṃ idanti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
  }
]