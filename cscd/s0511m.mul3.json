  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Brahmunā ",
      " yācito santo, koṇḍañño dvipaduttamo;"
    ],
    "innerText": "Brahmunā yācito santo, koṇḍañño dvipaduttamo;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vatti cakkaṃ mahāvīro, devānaṃ nagaruttame."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
