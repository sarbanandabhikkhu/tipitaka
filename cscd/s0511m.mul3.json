    ],
    "innerText": "Dasavassasahassāni, agāraṃ ajjha so vasi [agāramajjhe ca so vasi (syā.)];"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Suci suruci subho ca, tayo pāsādamuttamā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
