    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Dhātuvitthārikaṃ ",
      " āsi, gotamassa mahesino;"
    ],
    "innerText": "Dhātuvitthārikaṃ āsi, gotamassa mahesino;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pāṇīnaṃ anukampāya, ahu porāṇikaṃ tadāti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Dhātubhājanīyakathā niṭṭhitā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Buddhavaṃsoniṭṭhito."
  }
]