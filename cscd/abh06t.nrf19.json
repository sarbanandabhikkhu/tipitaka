[
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "chapter"
    },
    "innerText": "19. Ekūnavīsatimo paricchedo"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "Kaṅkhāvitaraṇavisuddhiniddeso"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1227"
      },
      "."
    ],
    "innerText": "1227."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Etassa ",
      " nāmarūpassa, jānitvā hetupaccaye;"
    ],
    "innerText": "Etassa nāmarūpassa, jānitvā hetupaccaye;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kaṅkhā tīsu panaddhāsu, vitaritvā ṭhitaṃ pana."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1228"
      },
      "."
    ],
    "innerText": "1228."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kaṅkhāvitaraṇaṃ nāma, ñāṇaṃ taṃ samudīritaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Taṃ sampādetukāmena, atthakāmena bhikkhunā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1229"
      },
      "."
    ],
    "innerText": "1229."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Nāmarūpassa ko hetu, konu vā paccayo bhave;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Āvajjitvā tamiccevaṃ, rūpakāyassa tāvade."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1230"
      },
      "."
    ],
    "innerText": "1230."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kesā lomā nakhā dantā, taco maṃsaṃ nahāru ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aṭṭhimiñjañca vakkañca, hadayaṃ yakanampi ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1231"
      },
      "."
    ],
    "innerText": "1231."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Iccevamādibāttiṃsa-koṭṭhāsapaccayassa hi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pariggaṇhati kāyassa, manasā hetupaccaye."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1232"
      },
      "."
    ],
    "innerText": "1232."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Avijjā ",
      " taṇhupādānaṃ, kammaṃ hetu catubbidho;"
    ],
    "innerText": "Avijjā taṇhupādānaṃ, kammaṃ hetu catubbidho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Etassa rūpakāyassa, āhāro paccayo mato."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1233"
      },
      "."
    ],
    "innerText": "1233."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Janako hetu akkhāto,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Paccayo anupālako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Hetvaṅkurassa bījaṃ tu,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Paccayā pathavādayo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1234"
      },
      "."
    ],
    "innerText": "1234."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Itime pañca dhammā hi, hetupaccayataṃ gatā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Avijjādayo tayo tattha, mātāva upanissayā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1235"
      },
      "."
    ],
    "innerText": "1235."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Janakaṃ pana kammaṃ tu, puttassa hi pitā viya;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dhātī viya kumārassa, āhāro dhārako bhave."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1236"
      },
      "."
    ],
    "innerText": "1236."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Iccevaṃ ",
      " rūpakāyassa, so paccayapariggahaṃ;"
    ],
    "innerText": "Iccevaṃ rūpakāyassa, so paccayapariggahaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Katvā punapi ‘‘cakkhuñca, rūpamālokameva ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1237"
      },
      "."
    ],
    "innerText": "1237."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Paṭicca cakkhuviññāṇaṃ, hoti’’iccevamādinā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Nayena nāmakāyassa, paccayaṃ parigaṇhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1238"
      },
      "."
    ],
    "innerText": "1238."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "So evaṃ nāmarūpassa, vuttiṃ disvāna paccayā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yathā etarahidaṃ tu, atītepi tathevidaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1239"
      },
      "."
    ],
    "innerText": "1239."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Paccayā ca pavattittha, tathevānāgatepi ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pavattissati addhāsu, tīsvevaṃ anupassati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1240"
      },
      "."
    ],
    "innerText": "1240."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Tassevaṃ ",
      " passato yā sā, pubbante pañcadhā tathā;"
    ],
    "innerText": "Tassevaṃ passato yā sā, pubbante pañcadhā tathā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aparante siyā kaṅkhā, pañcadhā samudīritā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1241"
      },
      "."
    ],
    "innerText": "1241."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Paccuppannepi addhāne, chabbidhā parikittitā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sabbā cānavasesāva, yogino sā pahiyyati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1242"
      },
      "."
    ],
    "innerText": "1242."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Eko kammavipākānaṃ, vasenāpi ca paṇḍito;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Etassa nāmarūpassa, paccayaṃ parigaṇhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1243"
      },
      "."
    ],
    "innerText": "1243."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kammaṃ catubbidhaṃ diṭṭha-dhammavedaniyaṃ tathā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Upapajjāparāpariyā-hosikammavasā pana."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "children": [
      "Tattha ",
      " ekajavanavīthiyaṃ sattasu cittesu kusalā vā akusalā vā paṭhamajavanacetanā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "diṭṭhadhammavedanīyakammaṃ"
      },
      " nāma. Taṃ imasmiṃyeva attabhāve vipākaṃ deti, tathā asakkontaṃ pana ‘‘ahosikammaṃ nāhosi kammavipāko, na bhavissati kammavipāko, natthi kammavipāko’’ti imassa tikassa vasena ahosikammaṃ nāma hoti. Atthasādhikā pana sattamajavanacetanā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "upapajjavedanīyakammaṃ"
      },
      " nāma. Tamanantare attabhāve vipākaṃ deti, tathā asakkontaṃ vuttanayena ahosikammaṃ nāma hoti. Ubhinnamantare pañcajavanacetanā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "aparāpariyavedanīyakammaṃ"
      },
      " nāma. Tamanāgate yadā okāsaṃ labhati, tadā vipākaṃ deti, sati saṃsārappavattiyā ahosikammaṃ nāma na hoti."
    ],
    "innerText": "Tattha ekajavanavīthiyaṃ sattasu cittesu kusalā vā akusalā vā paṭhamajavanacetanā diṭṭhadhammavedanīyakammaṃ nāma. Taṃ imasmiṃyeva attabhāve vipākaṃ deti, tathā asakkontaṃ pana ‘‘ahosikammaṃ nāhosi kammavipāko, na bhavissati kammavipāko, natthi kammavipāko’’ti imassa tikassa vasena ahosikammaṃ nāma hoti. Atthasādhikā pana sattamajavanacetanā upapajjavedanīyakammaṃ nāma. Tamanantare attabhāve vipākaṃ deti, tathā asakkontaṃ vuttanayena ahosikammaṃ nāma hoti. Ubhinnamantare pañcajavanacetanā aparāpariyavedanīyakammaṃ nāma. Tamanāgate yadā okāsaṃ labhati, tadā vipākaṃ deti, sati saṃsārappavattiyā ahosikammaṃ nāma na hoti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1244"
      },
      "."
    ],
    "innerText": "1244."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Aparaṃ ",
      " catubbidhaṃ kammaṃ, garukaṃ bahulampi ca;"
    ],
    "innerText": "Aparaṃ catubbidhaṃ kammaṃ, garukaṃ bahulampi ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Āsannañca kaṭattā ca, kammanti samudīritaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1245"
      },
      "."
    ],
    "innerText": "1245."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Aññaṃ catubbidhaṃ kammaṃ, janakaṃ upathambhakaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tathūpapīḷakaṃ kamma-mupaghātakameva ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "children": [
      "Tattha ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "janakaṃ"
      },
      " nāma kusalaṃ vā akusalaṃ vā kammaṃ paṭisandhiyampi pavattepi rūpārūpavipākakkhandhe janeti. ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Upatthambhakaṃ"
      },
      " pana vipākaṃ janetuṃ na sakkoti, aññena kammena dinnāya paṭisandhiyā janite vipāke uppajjanakasukhadukkhaṃ upatthambheti, addhānaṃ pavatteti. ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Upapīḷakaṃ"
      },
      " pana aññena kammena dinnāya paṭisandhiyā janite vipāke uppajjanakasukhadukkhaṃ pīḷeti bādhati, addhānaṃ pavattituṃ na deti. ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Upaghātakaṃ"
      },
      " pana sayaṃ kusalampi akusalampi samānaṃ ",
      " aññaṃ dubbalakammaṃ ghātetvā tassa vipākaṃ paṭibāhitvā attano vipākassa okāsaṃ karoti. Evaṃ pana kammena okāse kate taṃvipākamuppannaṃ nāma hoti. Iti imaṃ dvādasavidhaṃ kammaṃ kammavaṭṭe pakkhipitvā ",
      " evameko kammavipākavasena nāmarūpassa paccayapariggahaṃ karoti."
    ],
    "innerText": "Tattha janakaṃ nāma kusalaṃ vā akusalaṃ vā kammaṃ paṭisandhiyampi pavattepi rūpārūpavipākakkhandhe janeti. Upatthambhakaṃ pana vipākaṃ janetuṃ na sakkoti, aññena kammena dinnāya paṭisandhiyā janite vipāke uppajjanakasukhadukkhaṃ upatthambheti, addhānaṃ pavatteti. Upapīḷakaṃ pana aññena kammena dinnāya paṭisandhiyā janite vipāke uppajjanakasukhadukkhaṃ pīḷeti bādhati, addhānaṃ pavattituṃ na deti. Upaghātakaṃ pana sayaṃ kusalampi akusalampi samānaṃ aññaṃ dubbalakammaṃ ghātetvā tassa vipākaṃ paṭibāhitvā attano vipākassa okāsaṃ karoti. Evaṃ pana kammena okāse kate taṃvipākamuppannaṃ nāma hoti. Iti imaṃ dvādasavidhaṃ kammaṃ kammavaṭṭe pakkhipitvā evameko kammavipākavasena nāmarūpassa paccayapariggahaṃ karoti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "innerText": "Iti evaṃ kammavipākavaṭṭavasena nāmarūpassa pavattiṃ disvā ‘‘yathā idaṃ etarahi, evamatītepi addhāne kammavipākavasena paccayato pavattittha, anāgatepi pavattissatī’’ti iti kammañceva vipāko cāti kammavipākavasena loko pavattatīti taṃ samanupassati. Tassevaṃ samanupassato sabbā soḷasavidhā kaṅkhā pahiyyati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1246"
      },
      "."
    ],
    "innerText": "1246."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Hetuphalassa sambandhavaseneva pavattati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kevalaṃ nāmarūpanti, sammā samanupassati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1247"
      },
      "."
    ],
    "innerText": "1247."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Evaṃ kāraṇato uddhaṃ, kāraṇaṃ na ca passati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pākapavattito uddhaṃ, na pākapaṭivedakaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "innerText": "Tenāhu porāṇā –"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1248"
      },
      "."
    ],
    "innerText": "1248."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘‘Kammassa kārako natthi, vipākassa ca vedako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Suddhadhammā pavattanti, evetaṃ sammadassanaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1249"
      },
      "."
    ],
    "innerText": "1249."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Evaṃ ",
      " kamme vipāke ca, vattamāne sahetuke;"
    ],
    "innerText": "Evaṃ kamme vipāke ca, vattamāne sahetuke;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bījarukkhādikānaṃva, pubbā koṭi na nāyati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1250"
      },
      "."
    ],
    "innerText": "1250."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Anāgatepi saṃsāre, appavatti na dissati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Etamatthamanaññāya, titthiyā asayaṃvasī."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1251"
      },
      "."
    ],
    "innerText": "1251."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sattasaññaṃ gahetvāna, sassatucchedadassino;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dvāsaṭṭhidiṭṭhiṃ gaṇhanti, aññamaññavirodhino."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1252"
      },
      "."
    ],
    "innerText": "1252."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Diṭṭhibandhanabaddhā te, taṇhāsotena vuyhare;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Taṇhāsotena vuyhantā, na te dukkhā pamuccare."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1253"
      },
      "."
    ],
    "innerText": "1253."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Evametaṃ abhiññāya, bhikkhu buddhassa sāvako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Gambhīraṃ nipuṇaṃ suññaṃ, paccayaṃ paṭivijjhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1254"
      },
      "."
    ],
    "innerText": "1254."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Kammaṃ ",
      " natthi vipākamhi, pāko kamme na vijjati;"
    ],
    "innerText": "Kammaṃ natthi vipākamhi, pāko kamme na vijjati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aññamaññaṃ ubho suññā, na ca kammaṃ vinā phalaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1255"
      },
      "."
    ],
    "innerText": "1255."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Yathā na sūriye aggi, na maṇimhi na gomaye;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Na tesaṃ bahi so atthi, sambhārehi ca jāyati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1256"
      },
      "."
    ],
    "innerText": "1256."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tathā na anto kammassa, vipāko upalabbhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bahiddhāpi na kammassa, na kammaṃ tattha vijjati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1257"
      },
      "."
    ],
    "innerText": "1257."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Phalena ",
      " suññaṃ taṃ kammaṃ, phalaṃ kamme na vijjati;"
    ],
    "innerText": "Phalena suññaṃ taṃ kammaṃ, phalaṃ kamme na vijjati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kammañca kho upādāya, tato nibbattate phalaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1258"
      },
      "."
    ],
    "innerText": "1258."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Na hettha devo brahmā vā,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Saṃsārassatthi kārako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Suddhadhammā pavattanti,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Hetusambhārapaccayā’’ti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1259"
      },
      "."
    ],
    "innerText": "1259."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Evaṃ nānappakārehi, nāmarūpassa paccayaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pariggahetvā addhāsu, taritvā kaṅkhamuṭṭhitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1260"
      },
      "."
    ],
    "innerText": "1260."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Kaṅkhāvitaraṇaṃ"
      },
      " nāma, ñāṇaṃ taṃ samudīritaṃ;"
    ],
    "innerText": "Kaṅkhāvitaraṇaṃ nāma, ñāṇaṃ taṃ samudīritaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dhammaṭṭhiti yathābhūtaṃ, taṃ sammādassanantipi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1261"
      },
      "."
    ],
    "innerText": "1261."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Iminā pana ñāṇena,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Saṃyutto buddhasāsane;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Hoti laddhapatiṭṭhova,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sotāpanno hi cūḷako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "1262"
      },
      "."
    ],
    "innerText": "1262."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tasmā sapañño pana atthakāmo,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Yo nāmarūpassa hetupaccayāni;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Pariggahaṃ sādhu karoti dhīro,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Khippaṃ sa nibbānapuraṃ upeti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Iti abhidhammāvatāre kaṅkhāvitaraṇavisuddhiniddeso nāma"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Ekūnavīsatimo paricchedo."
  }
]