    "innerText": "1. Saṅgahavāro"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Yaṃ ",
      " loko pūjayate, salokapālo sadā namassati ca;"
    ],
    "innerText": "Yaṃ loko pūjayate, salokapālo sadā namassati ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tasseta sāsanavaraṃ, vidūhi ñeyyaṃ naravarassa."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dvādasa padāni suttaṃ, taṃ sabbaṃ byañjanañca attho ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Taṃ viññeyyaṃ ubhayaṃ, ko attho byañjanaṃ katamaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Soḷasahārā netti ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[nettī (ka.)]"
      },
      ", pañcanayā sāsanassa pariyeṭṭhi;"
    ],
    "innerText": "Soḷasahārā netti [nettī (ka.)], pañcanayā sāsanassa pariyeṭṭhi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Aṭṭhārasamūlapadā, mahakaccānena ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[mahākaccānena (sī.)]"
      },
      " niddiṭṭhā."
    ],
    "innerText": "Aṭṭhārasamūlapadā, mahakaccānena [mahākaccānena (sī.)] niddiṭṭhā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Hārā ",
      " byañjanavicayo, suttassa nayā tayo ca suttattho;"
    ],
    "innerText": "Hārā byañjanavicayo, suttassa nayā tayo ca suttattho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ubhayaṃ pariggahītaṃ, vuccati suttaṃ yathāsuttaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Yā ceva desanā yañca, desitaṃ ubhayameva viññeyyaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tatrāyamānupubbī, navavidhasuttantapariyeṭṭhīti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Saṅgahavāro."
  }
]