    "innerText": "32."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yadā’dito ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "dakkhiṇantikā,"
      }
    ],
    "innerText": "Yadā’dito dakkhiṇantikā,"
