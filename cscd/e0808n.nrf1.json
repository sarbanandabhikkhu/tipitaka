    ],
    "innerText": "Chaṭṭho’khilalahu,jo vā,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Gayutā’ññe,cha’ggaṇā,na jo visame,;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ariyāya’ntaḍḍhe lo, chaṭṭho,’nte go,gaṇā cha’ññe."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "title"
    },
    "innerText": "Yatiniyama"
  },
  {
