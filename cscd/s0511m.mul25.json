    "innerText": "40."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Sumaṅgalo ",
      " ghaṭikāro ca, ahesuṃ aggupaṭṭhakā;"
    ],
    "innerText": "Sumaṅgalo ghaṭikāro ca, ahesuṃ aggupaṭṭhakā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
