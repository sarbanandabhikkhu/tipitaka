  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Evaṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[etaṃ (sī. syā. kaṃ. pī.)]"
      },
      " visesato ñatvā, appamādamhi paṇḍitā;"
    ],
    "innerText": "Evaṃ [etaṃ (sī. syā. kaṃ. pī.)] visesato ñatvā, appamādamhi paṇḍitā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Appamāde pamodanti, ariyānaṃ gocare ratā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "23"
      },
      "."
    ],
    "innerText": "23."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Te jhāyino sātatikā, niccaṃ daḷhaparakkamā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Phusanti dhīrā nibbānaṃ, yogakkhemaṃ anuttaraṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "24"
      },
      "."
    ],
    "innerText": "24."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Uṭṭhānavato satīmato ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[satimato (sī. syā. ka.)]"
      },
      ", sucikammassa nisammakārino;"
    ],
    "innerText": "Uṭṭhānavato satīmato [satimato (sī. syā. ka.)], sucikammassa nisammakārino;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Saññatassa dhammajīvino, appamattassa ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[apamattassa (?)]"
      },
      " yasobhivaḍḍhati."
    ],
    "innerText": "Saññatassa dhammajīvino, appamattassa [apamattassa (?)] yasobhivaḍḍhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "25"
      },
      "."
    ],
    "innerText": "25."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Uṭṭhānenappamādena ",
      ", saṃyamena damena ca;"
    ],
    "innerText": "Uṭṭhānenappamādena , saṃyamena damena ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dīpaṃ kayirātha medhāvī, yaṃ ogho nābhikīrati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "26"
      },
      "."
    ],
    "innerText": "26."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pamādamanuyuñjanti, bālā dummedhino janā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Appamādañca medhāvī, dhanaṃ seṭṭhaṃva rakkhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "27"
      },
      "."
    ],
    "innerText": "27."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Mā pamādamanuyuñjetha, mā kāmaratisanthavaṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[sandhavaṃ (ka)]"
      },
      ";"
    ],
    "innerText": "Mā pamādamanuyuñjetha, mā kāmaratisanthavaṃ [sandhavaṃ (ka)];"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Appamatto hi jhāyanto, pappoti vipulaṃ sukhaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "28"
      },
      "."
    ],
    "innerText": "28."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pamādaṃ appamādena, yadā nudati paṇḍito;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Paññāpāsādamāruyha, asoko sokiniṃ pajaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Pabbataṭṭhova bhūmaṭṭhe ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[bhummaṭṭhe (sī. syā.)]"
      },
      ", dhīro bāle avekkhati."
    ],
    "innerText": "Pabbataṭṭhova bhūmaṭṭhe [bhummaṭṭhe (sī. syā.)], dhīro bāle avekkhati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "29"
      },
      "."
    ],
    "innerText": "29."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Appamatto ",
      " pamattesu, suttesu bahujāgaro;"
    ],
    "innerText": "Appamatto pamattesu, suttesu bahujāgaro;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Abalassaṃva ",
      " sīghasso, hitvā yāti sumedhaso."
    ],
    "innerText": "Abalassaṃva sīghasso, hitvā yāti sumedhaso."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "30"
      },
      "."
