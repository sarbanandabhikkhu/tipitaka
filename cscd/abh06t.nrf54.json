  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dvattiṃseva padānettha, desitāni sarūpato;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tāni yevāpanakehi, chattiṃseva bhavanti ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
