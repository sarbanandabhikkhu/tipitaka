[
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "chapter"
    },
    "innerText": "Samuṭṭhānasīsasaṅkhepo"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "Samuṭṭhānassuddānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "257"
      },
      "."
    ],
    "innerText": "257."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Aniccā ",
      " sabbe saṅkhārā, dukkhānattā ca saṅkhatā;"
    ],
    "innerText": "Aniccā sabbe saṅkhārā, dukkhānattā ca saṅkhatā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Nibbānañceva paññatti, anattā iti nicchayā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Buddhacande anuppanne, buddhādicce anuggate;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tesaṃ sabhāgadhammānaṃ, nāmamattaṃ na nāyati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dukkaraṃ vividhaṃ katvā, pūrayitvāna pāramī;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Uppajjanti mahāvīrā, cakkhubhūtā sabrahmake."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Te desayanti saddhammaṃ, dukkhahāniṃ sukhāvahaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aṅgīraso sakyamuni, sabbabhūtānukampako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sabbasattuttamo sīho, piṭake tīṇi desayi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Suttantamabhidhammañca, vinayañca mahāguṇaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Evaṃ nīyati saddhammo, vinayo yadi tiṭṭhati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ubhato ca vibhaṅgāni, khandhakā yā ca mātikā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Mālā suttaguṇeneva, parivārena ganthitā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tasseva parivārassa, samuṭṭhānaṃ niyato kataṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Sambhedaṃ ",
      " nidānaṃ caññaṃ, sutte dissanti upari;"
    ],
    "innerText": "Sambhedaṃ nidānaṃ caññaṃ, sutte dissanti upari;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tasmā sikkhe parivāraṃ, dhammakāmo supesaloti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "Terasasamuṭṭhānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Vibhaṅge dvīsu paññattaṃ, uddisanti uposathe;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pavakkhāmi samuṭṭhānaṃ, yathāñāyaṃ suṇātha me."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Pārājikaṃ ",
      " yaṃ paṭhamaṃ, dutiyañca tato paraṃ;"
    ],
    "innerText": "Pārājikaṃ yaṃ paṭhamaṃ, dutiyañca tato paraṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sañcarittānubhāsanañca, atirekañca cīvaraṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Lomāni padasodhammo, bhūtaṃ saṃvidhānena ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Theyyadesanacorī ca, ananuññātāya terasa."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Terasete samuṭṭhāna nayā, viññūhi cintitā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ekekasmiṃ samuṭṭhāne, sadisā idha dissare."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "1. Paṭhamapārājikasamuṭṭhānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "258"
      },
      "."
    ],
    "innerText": "258."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Methunaṃ ",
      " sukkasaṃsaggo, aniyatā paṭhamikā;"
    ],
    "innerText": "Methunaṃ sukkasaṃsaggo, aniyatā paṭhamikā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pubbūpaparipācitā, raho bhikkhuniyā saha."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sabhojane raho dve ca, aṅguli udake hasaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Pahāre ",
      " uggire ceva, tepaññāsā ca sekhiyā."
    ],
    "innerText": "Pahāre uggire ceva, tepaññāsā ca sekhiyā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Adhakkhagāmāvassutā, talamaṭṭhañca suddhikā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vassaṃvuṭṭhā ca ovādaṃ, nānubandhe pavattiniṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Chasattati ime sikkhā, kāyamānasikā katā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sabbe ekasamuṭṭhānā, paṭhamaṃ pārājikaṃ yathā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Paṭhamapārājikasamuṭṭhānaṃ niṭṭhitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "2. Dutiyapārājikasamuṭṭhānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "259"
      },
      "."
    ],
    "innerText": "259."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Adinnaṃ ",
      " viggahuttari, duṭṭhullā attakāminaṃ;"
    ],
    "innerText": "Adinnaṃ viggahuttari, duṭṭhullā attakāminaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Amūlā aññabhāgiyā, aniyatā dutiyikā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Acchinde pariṇāmane, musā omasapesuṇā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Duṭṭhullā pathavīkhaṇe, bhūtaṃ aññāya ujjhāpe."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Nikkaḍḍhanaṃ ",
      " siñcanañca, āmisahetu bhuttāvī;"
    ],
    "innerText": "Nikkaḍḍhanaṃ siñcanañca, āmisahetu bhuttāvī;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ehi anādari bhiṃsā, apanidhe ca jīvitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Jānaṃ sappāṇakaṃ kammaṃ, ūnasaṃvāsanāsanā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sahadhammikavilekhā, moho amūlakena ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Kukkuccaṃ dhammikaṃ cīvaraṃ, datvā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[kukkuccaṃ dhammikaṃ cīvaraṃ (sī.), kukkuccaṃ dhammikaṃ datvā (syā.)]"
      },
      " pariṇāmeyya puggale;"
    ],
    "innerText": "Kukkuccaṃ dhammikaṃ cīvaraṃ, datvā [kukkuccaṃ dhammikaṃ cīvaraṃ (sī.), kukkuccaṃ dhammikaṃ datvā (syā.)] pariṇāmeyya puggale;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kiṃ te akālaṃ acchinde, duggahī nirayena ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Gaṇaṃ vibhaṅgaṃ dubbalaṃ, kathināphāsupassayaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Akkosacaṇḍī maccharī, gabbhinī ca pāyantiyā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Dvevassaṃ ",
      " sikkhā saṅghena, tayo ceva gihīgatā;"
    ],
    "innerText": "Dvevassaṃ sikkhā saṅghena, tayo ceva gihīgatā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kumāribhūtā tisso ca, ūnadvādasasammatā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Alaṃ tāva sokāvāsaṃ, chandā anuvassā ca dve;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sikkhāpadā sattatime, samuṭṭhānā tikā katā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kāyacittena na vācā, vācācittaṃ na kāyikaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tīhi dvārehi jāyanti, pārājikaṃ dutiyaṃ yathā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Dutiyapārājikasamuṭṭhānaṃ niṭṭhitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "3. Sañcarittasamuṭṭhānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "260"
      },
      "."
    ],
    "innerText": "260."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Sañcarī ",
      " kuṭi vihāro, dhovanañca paṭiggaho;"
    ],
    "innerText": "Sañcarī kuṭi vihāro, dhovanañca paṭiggaho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Viññattuttari abhihaṭṭhuṃ, ubhinnaṃ dūtakena ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Kosiyā suddhadvebhāgā, chabbassāni nisīdanaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Riñcanti rūpikā ceva, ubho nānappakārakā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Ūnabandhanavassikā, suttaṃ vikappanena ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Dvāradānasibbāni ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[dvāradānasibbinī (sī. syā.)]"
      },
      " ca, pūvapaccayajoti ca."
    ],
    "innerText": "Dvāradānasibbāni [dvāradānasibbinī (sī. syā.)] ca, pūvapaccayajoti ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Ratanaṃ sūci mañco ca, tūlaṃ nisīdanakaṇḍu ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vassikā ca sugatena, viññatti aññaṃ cetāpanā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dve saṅghikā mahājanikā, dve puggalalahukā garu;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dve vighāsā sāṭikā ca, samaṇacīvarena ca."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Samapaññāsime dhammā, chahi ṭhānehi jāyare;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kāyato na vācācittā, vācato na kāyamanā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Kāyavācā na ca cittā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[na cittato (sī. syā.)]"
      },
      ", kāyacittā na vācikā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[na vācato (sī. syā.)]"
      },
      ";"
    ],
    "innerText": "Kāyavācā na ca cittā [na cittato (sī. syā.)], kāyacittā na vācikā [na vācato (sī. syā.)];"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Vācācittā ",
      " na kāyena, tīhi dvārehi ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[tīhi ṭhānehi (syā.)]"
      },
      " jāyare."
    ],
    "innerText": "Vācācittā na kāyena, tīhi dvārehi [tīhi ṭhānehi (syā.)] jāyare."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Chasamuṭṭhānikā ete, sañcarittena sādisā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Sañcarittasamuṭṭhānaṃ niṭṭhitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subhead"
    },
    "innerText": "4. Samanubhāsanāsamuṭṭhānaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "261"
      },
      "."
    ],
    "innerText": "261."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Bhedānuvattadubbaca ",
      ", dūsaduṭṭhulladiṭṭhi ca;"
    ],
    "innerText": "Bhedānuvattadubbaca , dūsaduṭṭhulladiṭṭhi ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Chandaṃ ujjagghikā dve ca, dve ca saddā na byāhare."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
