  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tihetukāmakriyato, vītarāgānamappanā."
  },
  {
    "tagName": "p",
    "attributes": {
