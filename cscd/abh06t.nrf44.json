    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Nimittagatikammāni ",
      ", kammamevātha gocarā;"
    ],
    "innerText": "Nimittagatikammāni , kammamevātha gocarā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Paṭisandhibhavaṅgānaṃ, cutiyā ca yathārahaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
