    },
    "innerText": "Ganthārambhakathā"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Buddhaṃ ",
      " dhammañca saṅghañca, vippasannena cetasā;"
    ],
    "innerText": "Buddhaṃ dhammañca saṅghañca, vippasannena cetasā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vanditvā vandanāmāna, pūjāsakkārabhājanaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Theravaṃsappadīpānaṃ, thirānaṃ vinayakkame;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pubbācariyasīhānaṃ, namo katvā katañjalī."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pāmokkhaṃ anavajjānaṃ, dhammānaṃ yaṃ mahesinā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Mukhaṃ mokkhappavesāya, pātimokkhaṃ pakāsitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Sūratena ",
      " nivātena, sucisallekhavuttinā;"
    ],
    "innerText": "Sūratena nivātena, sucisallekhavuttinā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Vinayācārayuttena, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "soṇattherena"
      },
      " yācito."
    ],
    "innerText": "Vinayācārayuttena, soṇattherena yācito."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tattha sañjātakaṅkhānaṃ, bhikkhūnaṃ tassa vaṇṇanaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kaṅkhāvitaraṇatthāya, paripuṇṇavinicchayaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Mahāvihāravāsīnaṃ, vācanāmagganissitaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Vattayissāmi nāmena, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "kaṅkhāvitaraṇiṃ"
      },
      " subhanti."
    ],
    "innerText": "Vattayissāmi nāmena, kaṅkhāvitaraṇiṃ subhanti."
  }
]