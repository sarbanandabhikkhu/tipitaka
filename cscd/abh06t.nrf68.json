  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Paññāpīyanti nāmāti, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
