  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘‘Atha ayyā jinadattā, āgacchī gocarāya caramānā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Tātakulaṃ ",
      " vinayadharī, bahussutā sīlasampannā."
    ],
    "innerText": "Tātakulaṃ vinayadharī, bahussutā sīlasampannā."
  },
