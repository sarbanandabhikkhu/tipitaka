    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Atha maṃ bhaṇatī tāto, ‘idheva puttaka ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
