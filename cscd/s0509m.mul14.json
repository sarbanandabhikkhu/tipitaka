    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Tiṃsativassamhi ",
      " mato, sākaṭikakulamhi dārikā jātā;"
    ],
    "innerText": "‘‘Tiṃsativassamhi mato, sākaṭikakulamhi dārikā jātā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
