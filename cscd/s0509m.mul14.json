    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " ‘‘Nagaramhi erakacche ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[erakakacche (syā. ka.)]"
      },
      ", suvaṇṇakāro ahaṃ pahūtadhano;"
    ],
    "innerText": "‘‘Nagaramhi erakacche [erakakacche (syā. ka.)], suvaṇṇakāro ahaṃ pahūtadhano;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yobbanamadena matto so, paradāraṃ asevihaṃ."
