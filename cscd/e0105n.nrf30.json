  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dīghāgamassa dasabala-guṇagaṇaparidīpanassa aṭṭhakathaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Yaṃ ārabhiṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
