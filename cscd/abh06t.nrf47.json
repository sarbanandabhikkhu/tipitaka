  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tecattālīsa cekūnacattālīsa yathākkamaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bhavantekūnatālīsa, pañcattiṃsa ca rūpisu."
  },
  {
