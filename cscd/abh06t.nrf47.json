  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Kaṅkhādiṭṭhiyutā ",
      " pañca, sekkhānaṃ natthi mānasā;"
    ],
    "innerText": "Kaṅkhādiṭṭhiyutā pañca, sekkhānaṃ natthi mānasā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dosamūladvayañcāpi, natthānāgāmino pana."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
