  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Catupaññāsa paññāsa, paññāsadvayahīnakā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kāmesu tesaṃ sambhonti, catutālīsa cakkamā."
  },
