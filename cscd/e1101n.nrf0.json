    },
    "children": [
      " Satthuppasatthacaraṇaṃ ",
      " saraṇaṃ janānaṃ,"
    ],
    "innerText": "Satthuppasatthacaraṇaṃ saraṇaṃ janānaṃ,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Brahmādimoḷi maṇiraṃsi samāvahantaṃ,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha3"
    },
    "innerText": "Paṅkeruhābhamudukomalacāruvaṇṇaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vandāmi cakkavaralakkhaṇamādadhānaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
