    "attributes": {
      "name": "class",
      "value": "book"
    },
    "innerText": "Abhidhammatthasaṅgaho"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subsubhead"
    },
    "innerText": "Ganthārambhakathā"
  },
  {
