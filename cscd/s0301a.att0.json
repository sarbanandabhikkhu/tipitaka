[
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Namo tassa bhagavato arahato sammāsambuddhassa"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "nikaya"
    },
    "innerText": "Saṃyuttanikāye"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "book"
    },
    "innerText": "Sagāthāvagga-aṭṭhakathā"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "subsubhead"
    },
    "innerText": "Ganthārambhakathā"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Karuṇāsītalahadayaṃ ",
      ", paññāpajjotavihatamohatamaṃ;"
    ],
    "innerText": "Karuṇāsītalahadayaṃ , paññāpajjotavihatamohatamaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Sanarāmaralokagaruṃ, vande ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "sugataṃ"
      },
      " gativimuttaṃ."
    ],
    "innerText": "Sanarāmaralokagaruṃ, vande sugataṃ gativimuttaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Buddhopi buddhabhāvaṃ, bhāvetvā ceva sacchikatvā ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Yaṃ upagato gatamalaṃ, vande tamanuttaraṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "dhammaṃ"
      },
      "."
    ],
    "innerText": "Yaṃ upagato gatamalaṃ, vande tamanuttaraṃ dhammaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sugatassa orasānaṃ, puttānaṃ mārasenamathanānaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Aṭṭhannampi samūhaṃ, sirasā vande ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "ariyasaṅghaṃ"
      },
      "."
    ],
    "innerText": "Aṭṭhannampi samūhaṃ, sirasā vande ariyasaṅghaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Iti ",
      " me pasannamatino, ratanattayavandanāmayaṃ puññaṃ;"
    ],
    "innerText": "Iti me pasannamatino, ratanattayavandanāmayaṃ puññaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yaṃ suvihatantarāyo, hutvā tassānubhāvena."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Saṃyuttavaggapaṭimaṇḍitassa, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "saṃyuttaāgamavarassa"
      },
      ";"
    ],
    "innerText": "Saṃyuttavaggapaṭimaṇḍitassa, saṃyuttaāgamavarassa;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Buddhānubuddhasaṃvaṇṇitassa, ñāṇappabhedajananassa."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Atthappakāsanatthaṃ, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "aṭṭhakathā"
      },
      " ādito vasisatehi;"
    ],
    "innerText": "Atthappakāsanatthaṃ, aṭṭhakathā ādito vasisatehi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pañcahi yā saṅgītā, anusaṅgītā ca pacchāpi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sīhaḷadīpaṃ pana ābhatātha, vasinā mahāmahindena;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ṭhapitā sīhaḷabhāsāya, dīpavāsīnamatthāya."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Apanetvāna ",
      " tatohaṃ, sīhaḷabhāsaṃ manoramaṃ bhāsaṃ;"
    ],
    "innerText": "Apanetvāna tatohaṃ, sīhaḷabhāsaṃ manoramaṃ bhāsaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tantinayānucchavikaṃ, āropento vigatadosaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Samayaṃ avilomento, therānaṃ theravaṃsadīpānaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sunipuṇavinicchayānaṃ, mahāvihāre nivāsīnaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Hitvā ",
      " punappunāgata-matthaṃ, atthaṃ pakāsayissāmi;"
    ],
    "innerText": "Hitvā punappunāgata-matthaṃ, atthaṃ pakāsayissāmi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sujanassa ca tuṭṭhatthaṃ, ciraṭṭhitatthañca dhammassa."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sāvatthipabhūtīnaṃ, nagarānaṃ vaṇṇanā katā heṭṭhā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Saṅgītīnaṃ dvinnaṃ, yā me atthaṃ vadantena."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Vitthāravasena sudaṃ, vatthūni ca yāni tattha vuttāni;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tesampi na idha bhiyyo, vitthārakathaṃ karissāmi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Suttānaṃ ",
      " pana atthā, na vinā vatthūhi ye pakāsanti;"
    ],
    "innerText": "Suttānaṃ pana atthā, na vinā vatthūhi ye pakāsanti;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tesaṃ pakāsanatthaṃ, vatthūnipi dassayissāmi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sīlakathā dhutadhammā, kammaṭṭhānāni ceva sabbāni;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Cariyāvidhānasahito, jhānasamāpattivitthāro."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sabbā ca abhiññāyo, paññāsaṅkalananicchayo ceva;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Khandhādhātāyatanindriyāni, ariyāni ceva cattāri."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Saccāni paccayākāradesanā, suparisuddhanipuṇanayā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Avimuttatantimaggā, vipassanābhāvanā ceva."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Iti pana sabbaṃ yasmā, ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "visuddhimagge"
      },
      " mayā suparisuddhaṃ;"
    ],
    "innerText": "Iti pana sabbaṃ yasmā, visuddhimagge mayā suparisuddhaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vuttaṃ tasmā bhiyyo, na taṃ idha vicārayissāmi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "‘‘Majjhe ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "visuddhimaggo,"
      },
      " esa catunnampi āgamānañhi;"
    ],
    "innerText": "‘‘Majjhe visuddhimaggo, esa catunnampi āgamānañhi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ṭhatvā pakāsayissati, tattha yathābhāsitamatthaṃ’’."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Icceva kato tasmā, tampi gahetvāna saddhimetāya;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Aṭṭhakathāya vijānatha, saṃyuttavinissitaṃ atthanti."
  }
]