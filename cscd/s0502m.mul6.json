  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yassindriyāni samathaṅgatāni ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[samathaṃ gatāni (sī. pī.)]"
      },
      ", assā yathā sārathinā sudantā;"
    ],
    "innerText": "Yassindriyāni samathaṅgatāni [samathaṃ gatāni (sī. pī.)], assā yathā sārathinā sudantā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pahīnamānassa anāsavassa, devāpi tassa pihayanti tādino."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
