    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Ramaṇīyāni ",
      " araññāni, yattha na ramatī jano;"
    ],
    "innerText": "Ramaṇīyāni araññāni, yattha na ramatī jano;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Vītarāgā ramissanti, na te kāmagavesino."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Arahantavaggo sattamo niṭṭhito."
  }
]