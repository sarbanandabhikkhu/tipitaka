    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yassāsavā ",
      " parikkhīṇā, āhāre ca anissito;"
    ],
    "innerText": "Yassāsavā parikkhīṇā, āhāre ca anissito;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Suññato animitto ca, vimokkho yassa gocaro;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ākāse va sakuntānaṃ, padaṃ tassa durannayaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
