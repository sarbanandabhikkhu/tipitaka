    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Uyyuñjanti ",
      " satīmanto, na nikete ramanti te;"
    ],
    "innerText": "Uyyuñjanti satīmanto, na nikete ramanti te;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Haṃsāva pallalaṃ hitvā, okamokaṃ jahanti te."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
