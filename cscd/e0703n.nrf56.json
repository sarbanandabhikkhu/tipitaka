  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tilokasundaraṃ nāma, sukumāraṃ kumārikaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
