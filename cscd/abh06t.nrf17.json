    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Iti abhidhammāvatāre abhiññārammaṇaniddeso nāma"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Sattarasamo paricchedo."
  }
]