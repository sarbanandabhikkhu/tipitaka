    "innerText": "1161."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Anāgataṃsañāṇassa ",
      ", anāgataṃsagocaraṃ;"
    ],
    "innerText": "Anāgataṃsañāṇassa , anāgataṃsagocaraṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "‘‘Ahaṃ devo bhavissāmi’’-ccevamajjhattagocaraṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
