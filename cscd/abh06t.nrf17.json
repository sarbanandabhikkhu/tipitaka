    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Yathākammupagañāṇaṃ"
      },
      ", parittādīsu pañcasu;"
    ],
    "innerText": "Yathākammupagañāṇaṃ, parittādīsu pañcasu;"
