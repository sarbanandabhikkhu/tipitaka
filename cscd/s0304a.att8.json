        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "leṇaṃ"
      },
      ". Tāyanaṭṭhena ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "tāṇaṃ"
      },
