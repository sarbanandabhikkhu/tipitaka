    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Khandhāyatanadhātādippabhedehi ",
      " yathārahaṃ;"
    ],
    "innerText": "Khandhāyatanadhātādippabhedehi yathārahaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Lakkhaṇapaccupaṭṭhāna-padaṭṭhānavibhāgato."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
