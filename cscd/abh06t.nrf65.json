    ],
    "innerText": "Taṇhāvijjānābhikaṃ taṃ, jarāmaraṇanemikaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sesākārādighaṭikaṃ, tibhavārathayojitaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
