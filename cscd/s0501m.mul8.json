  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Santussako ",
      " ca subharo ca, appakicco ca sallahukavutti;"
    ],
    "innerText": "Santussako ca subharo ca, appakicco ca sallahukavutti;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Santindriyo ca nipako ca, appagabbho kulesvananugiddho."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "3"
      },
      "."
    ],
    "innerText": "3."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Na ca khuddamācare kiñci, yena viññū pare upavadeyyuṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Sukhinova khemino hontu, sabbasattā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[sabbe sattā (sī. syā.)]"
      },
      " bhavantu sukhitattā."
    ],
    "innerText": "Sukhinova khemino hontu, sabbasattā [sabbe sattā (sī. syā.)] bhavantu sukhitattā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "4"
      },
      "."
    ],
    "innerText": "4."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Ye keci pāṇabhūtatthi, tasā vā thāvarā vanavasesā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Dīghā vā yeva mahantā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[mahanta (?)]"
      },
      ", majjhimā rassakā aṇukathūlā."
    ],
    "innerText": "Dīghā vā yeva mahantā [mahanta (?)], majjhimā rassakā aṇukathūlā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "5"
      },
      "."
    ],
    "innerText": "5."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Diṭṭhā vā yeva adiṭṭhā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[adiṭṭha (?)]"
      },
      ", ye va ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[ye ca (sī. syā. kaṃ. pī.)]"
      },
      " dūre vasanti avidūre;"
    ],
    "innerText": "Diṭṭhā vā yeva adiṭṭhā [adiṭṭha (?)], ye va [ye ca (sī. syā. kaṃ. pī.)] dūre vasanti avidūre;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Bhūtā va ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[vā (syā. kaṃ. pī. ka.)]"
      },
      " sambhavesī va ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[vā (sī. syā. kaṃ. pī.)]"
      },
      ", sabbasattā bhavantu sukhitattā."
    ],
    "innerText": "Bhūtā va [vā (syā. kaṃ. pī. ka.)] sambhavesī va [vā (sī. syā. kaṃ. pī.)], sabbasattā bhavantu sukhitattā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
