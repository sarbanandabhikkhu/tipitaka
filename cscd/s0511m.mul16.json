  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Sopi ",
      " buddho asamasamo, asītihatthamuggato;"
    ],
    "innerText": "Sopi buddho asamasamo, asītihatthamuggato;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Atirocati tejena, dasasahassimhi dhātuyā."
