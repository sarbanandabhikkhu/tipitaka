  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Aṭṭhavassasahassāni ",
      ", agāraṃ ajjha so vasi;"
    ],
    "innerText": "Aṭṭhavassasahassāni , agāraṃ ajjha so vasi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Arajo virajo sudassano, tayo pāsādamuttamā."
