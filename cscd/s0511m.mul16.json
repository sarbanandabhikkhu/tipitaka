    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Obhāsaṃ ",
      " dassayitvāna, vimalaṃ katvāna sāsanaṃ;"
    ],
    "innerText": "Obhāsaṃ dassayitvāna, vimalaṃ katvāna sāsanaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Cavi candova gagane, nibbuto so sasāvako."
