  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Subhaddo kaṭissaho ceva, ahesuṃ aggupaṭṭhakā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Sāḷiyā ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[sālisā (syā. kaṃ.)]"
      },
      " ca kaḷiyā ca, ahesuṃ aggupaṭṭhikā."
    ],
    "innerText": "Sāḷiyā [sālisā (syā. kaṃ.)] ca kaḷiyā ca, ahesuṃ aggupaṭṭhikā."
