[
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Namo tassa bhagavato arahato sammāsambuddhassa."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "book"
    },
    "innerText": "Sīmavisodhanī"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Varadaṃ ",
      " varadaṃ aggaṃ, diccaṃ diccaṃva vanajaṃ;"
    ],
    "innerText": "Varadaṃ varadaṃ aggaṃ, diccaṃ diccaṃva vanajaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bodhakaṃ bodhakaṃ pāṇaṃ, deva’devaṃ namāma’haṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Santaṃ santaṃ sadā vaṭṭaṃ, pajaṃ’bhave sutaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Nibbudaṃ nibbudaṃ tāraṃ, dhammavaraṃ namāma’haṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sutaṃ sutaṃ bhavane yaṃ, natanaṃ natanā’rahaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Narā’narā sadā’kaṃsu, saṅghavaraṃ namāma’haṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Icceva’maccantanatassaneyyaṃ, natayyamāno ratanattayaṃ yaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Puññābhivaḍḍhiṃ mahagghaṃ alatthaṃ, tassānubhāvena hatantarāyo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Vinaye sati ṭhite yaṃ, sāsanaṃ saṇṭhitaṃ bhave;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vinaye sati naṭṭhe yaṃ, vinaṭṭhaṃ sāsanaṃ bhave."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Tasmā dittaṃ mame’dāni, jotayitvāna sāsane;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bhinditvā dveḷhakaṃ vādaṃ, ekībhāvaṃ karoma’haṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Attukkaṃsana-bhāvañca, paravambhanakampica;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tesu cittaṃ apesetvā, sāsanassa suvuḍḍhiyā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Yadi me īdisaṃ cittaṃ, bhaveyyuṃ pāpasaṅkappā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Paccissaṃ niraye bhayaṃ, hīna’jjhāsayakāraṇā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Sāsanassa ",
      " sulabhaṃ’va patthayanto satā’cāro;"
    ],
    "innerText": "Sāsanassa sulabhaṃ’va patthayanto satā’cāro;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Saṃsandissamahaṃ dāni, ciraṃ saddhamma’suddhiyā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sudullabhaṃ labhitvāna, pabbajjaṃ jinasāsane;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sambuddhassa varaṃ vādaṃ, katvā dveḷhaka’mekikaṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sīmavisodhaniṃ nāma, nānāganthasamāhaṭaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Karissaṃ me nisāmentu, sādhavo kavipuṅgavā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pāḷiṃ aṭṭhakathañceva, mātikaṃ padabhājaniṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Ogāhetvāna taṃ sabbaṃ, punappunaṃ asesato."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Attanomatiganthesu, ṭīkāgaṇṭhipadesuca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Vinicchayavimatīsu, mātikā’ṭṭhakathāsupi."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Sabbaṃ asesakaṃ katvā, saṃsanditvāna ekato;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Pavattā vaṇṇanā esā, tosayantī vicakkhaṇeti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "innerText": "Buddhuppādo hi dullabho, tato pabbajjā ca upasampadā ca, vuttañhetaṃ"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Buddho ca dullabho loke, saddhammasavanampi ca;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Saṅgho ca dullabho loke, sappūrisā’tidullabhā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Dullabhañca manussattaṃ, buddhuppādo ca dullabho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Dullabhā khaṇasampatti, saddhammo paramadullabho’ti."
  }
]