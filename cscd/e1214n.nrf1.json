  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Pārakkhaṇamhi pā pāne brū vācāyaṃ viyattiyaṃ,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Bhā dittiyaṃ mā pamāṇe (atho) yā pāpuṇe (siyā;)"
