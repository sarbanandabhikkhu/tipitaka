  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Jhamu dāhe chamu adane irīya vattane’(pi ca),"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kira (dhātu) vikiraṇe giro nigiraṇā’disu;"
