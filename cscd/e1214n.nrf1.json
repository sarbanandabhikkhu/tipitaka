  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Bhaja sevāputhakkāre ruja roge aṭā’ṭane,"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kuṭacchede (ca) koṭille agā sajjhāyanā’disu;"
