    "children": [
      {
        "tagName": "font",
        "attributes": {
          "name": "style",
          "value": "vertical-align: inherit;"
        },
        "innerText": "''Kāsikasukhumāni dhāraya, abhiropehi "
      },
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
