    "attributes": {
      "name": "class",
      "value": "bodytext"
    },
    "children": [
      "Tattha ",
      " akkharesu okārantā aṭṭha sarā nāma. Tattheti vattate."
    ],
    "innerText": "Tattha akkharesu okārantā aṭṭha sarā nāma. Tattheti vattate."
