    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Cittavisuddhi"
      },
      " nāmāyaṃ, cittasaṃklesasodhano;"
    ],
    "innerText": "Cittavisuddhi nāmāyaṃ, cittasaṃklesasodhano;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Upacārappanābhedo, samatho pubbabhāgiyoti."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Iti nibbānavibhāge mūlavisuddhikathā niṭṭhitā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Tevīsatimo paricchedo."
  }
]