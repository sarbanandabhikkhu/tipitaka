    },
    "children": [
      " Yuñjantassa ",
      " panetesu, gocaresu catūsupi;"
    ],
    "innerText": "Yuñjantassa panetesu, gocaresu catūsupi;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Appenti anupubbena, āruppāpi catubbidhā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "919"
      },
