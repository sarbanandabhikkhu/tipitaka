  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Odātamanasā suddhā, vippasannamanāvilā’’ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[vippasannāmanāvilā (pī. ka.)]"
      },
      "."
    ],
    "innerText": "Odātamanasā suddhā, vippasannamanāvilā’’ [vippasannāmanāvilā (pī. ka.)]."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      "Bhiyyo ",
      " pañcasate ñatvā, vane kāpilavatthave;"
    ],
    "innerText": "Bhiyyo pañcasate ñatvā, vane kāpilavatthave;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Tato āmantayī satthā, sāvake sāsane rate."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "‘‘Devakāyā abhikkantā, te vijānātha bhikkhavo’’;"
