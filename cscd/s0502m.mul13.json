  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Ye jhānapasutā dhīrā, nekkhammūpasame ratā;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Devāpi tesaṃ pihayanti, sambuddhānaṃ satīmataṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
        },
        "innerText": "182"
      },
      "."
    ],
    "innerText": "182."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Kiccho ",
      " manussapaṭilābho, kicchaṃ maccāna jīvitaṃ;"
    ],
    "innerText": "Kiccho manussapaṭilābho, kicchaṃ maccāna jīvitaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Kicchaṃ saddhammassavanaṃ, kiccho buddhānamuppādo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
