      },
      ".",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "Ekāva"
      },
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "bld"
        },
        "innerText": "dukkaṭāpatti vuttā"
      },
      "ti vassaṃ anupagamanādipaccayā jātito ekāva dukkaṭāpatti vuttā."
    ],
    "innerText": "304.Ekāvadukkaṭāpatti vuttāti vassaṃ anupagamanādipaccayā jātito ekāva dukkaṭāpatti vuttā."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
