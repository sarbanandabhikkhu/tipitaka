    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Anupassanāpi ",
      " ñāṇanti, varañāṇena desitaṃ;"
    ],
    "innerText": "Anupassanāpi ñāṇanti, varañāṇena desitaṃ;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "So panevaṃ pajānāti, yogāvacaramāṇavo."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
