  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Na ",
      " taṃ kammaṃ kataṃ sādhu, yaṃ katvā anutappati;"
    ],
    "innerText": "Na taṃ kammaṃ kataṃ sādhu, yaṃ katvā anutappati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yassa assumukho rodaṃ, vipākaṃ paṭisevati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
