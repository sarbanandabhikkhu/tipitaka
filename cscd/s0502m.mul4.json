  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "children": [
      "Ḍahantaṃ bālamanveti, bhasmacchannova ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[bhasmāchannova (sī. pī. ka.)]"
      },
      " pāvako."
    ],
    "innerText": "Ḍahantaṃ bālamanveti, bhasmacchannova [bhasmāchannova (sī. pī. ka.)] pāvako."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
