  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Tañca ",
      " kammaṃ kataṃ sādhu, yaṃ katvā nānutappati;"
    ],
    "innerText": "Tañca kammaṃ kataṃ sādhu, yaṃ katvā nānutappati;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Yassa patīto sumano, vipākaṃ paṭisevati."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
