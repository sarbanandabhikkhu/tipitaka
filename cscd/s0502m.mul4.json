  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "innerText": "Aññā hi lābhūpanisā, aññā nibbānagāminī;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha2"
    },
    "innerText": "Evametaṃ abhiññāya, bhikkhu buddhassa sāvako;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Sakkāraṃ nābhinandeyya, vivekamanubrūhaye."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "centered"
    },
    "innerText": "Bālavaggo pañcamo niṭṭhito."
  }
]