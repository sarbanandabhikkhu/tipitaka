  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Asantaṃ ",
      " bhāvanamiccheyya ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[asantaṃ bhāvamiccheyya (syā.), asantabhāvanamiccheyya (ka.)]"
      },
      ", purekkhārañca bhikkhusu;"
    ],
    "innerText": "Asantaṃ bhāvanamiccheyya [asantaṃ bhāvamiccheyya (syā.), asantabhāvanamiccheyya (ka.)], purekkhārañca bhikkhusu;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Āvāsesu ca issariyaṃ, pūjā parakulesu ca."
