    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Ārakattārihantattā ",
      ", pāpākārakatoraho;"
    ],
    "innerText": "Ārakattārihantattā , pāpākārakatoraho;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Hata cakkārato pūjā, rahattā cārahaṃ name."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
