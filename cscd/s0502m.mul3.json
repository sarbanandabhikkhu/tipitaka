  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yathāpi ",
      " puppharāsimhā, kayirā mālāguṇe bahū;"
    ],
    "innerText": "Yathāpi puppharāsimhā, kayirā mālāguṇe bahū;"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Evaṃ jātena maccena, kattabbaṃ kusalaṃ bahuṃ."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "paranum"
