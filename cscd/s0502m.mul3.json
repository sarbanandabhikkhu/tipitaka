  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gatha1"
    },
    "children": [
      " Yathāpi bhamaro pupphaṃ, vaṇṇagandhamaheṭhayaṃ ",
      {
        "tagName": "span",
        "attributes": {
          "name": "class",
          "value": "note"
        },
        "innerText": "[vaṇṇagandhamapoṭhayaṃ (ka.)]"
      },
      ";"
    ],
    "innerText": "Yathāpi bhamaro pupphaṃ, vaṇṇagandhamaheṭhayaṃ [vaṇṇagandhamapoṭhayaṃ (ka.)];"
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "gathalast"
    },
    "innerText": "Paleti rasamādāya, evaṃ gāme munī care."
  },
  {
    "tagName": "p",
    "attributes": {
      "name": "class",
      "value": "hangnum"
    },
    "children": [
      {
        "tagName": "span",
